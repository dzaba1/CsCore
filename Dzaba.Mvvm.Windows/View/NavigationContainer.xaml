<UserControl x:Class="Dzaba.Mvvm.Windows.View.NavigationContainer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mvvmNavigation="clr-namespace:Dzaba.Mvvm.Windows.Navigation"
             xmlns:windows="clr-namespace:Dzaba.Mvvm.Windows"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             windows:ViewModel.Type="{x:Type mvvmNavigation:INavigationService}">
    <ContentControl Content="{Binding Path=ActiveView}"/>
</UserControl>
